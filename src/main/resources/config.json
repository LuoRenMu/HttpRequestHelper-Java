        "method": "get",
        "param": [
          {
            "name": "collection_id",
            "content": "{id}"
          },
          {
            "name": "order_type",
            "content": "2"
          }
        ]
      },
      "article": {
        "request": "https://bbs-api.miyoushe.com/post/api/getPostFull",
        "method": "get",
        "param": [
          {
            "name": "post_id",
            "content": "{id}"
          },
          {
            "name": "csm_source",
            "content": "search"
          }
        ]
      }
    },
    "account": {
      "access_device": {
        "request": "https://public-data-api.mihoyo.com/device-fp/api/getFp",
        "method": "post",
        "body": [
          {
            "name": "device_id",
            "content": ""
          }
        ]
      }
    }
